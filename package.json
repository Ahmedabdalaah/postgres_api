{
  "name": "postfres-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "lint": "eslint .",
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "test": "set ENV=test&& db-migrate --config ./database.json --e test up && npm run build & npm run jasmine  && db-migrate reset --config ./database.json --e test",
    "testdb-reset": "db-migrate reset --config ./database.json --e test",
    "testdb-up": "db-migrate --config ./database.json --e test up",
    "devdb-up": "db-migrate up --config ./database.json --e dev",
    "devdb-reset": "db-migrate reset --config ./database.json --e dev",
    "lint:fix": "eslint --fix",
    "jasmine": "jasmine",
    "jest": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write {,!(node_modules)/**/}*.ts",
    "build": "npx tsc",
    "eslint": "eslint {,!(node_modules)/**/}*.ts",
    "eslint:fix": "eslint --fix {,!(node_modules)/**/}*.ts",
    "prettier": "prettier --config .prettierrc {,!(node_modules)/**/}*.ts --write",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "prepare": "husky install",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "onchange {,!(node_modules)/**/}*.ts -- prettier --write {{changed}}",
    "pre-commit": " lint-staged",
    "fix-js": "eslint-auto-fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": " lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@stdlib/fs-read-file": "0.0.7",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/pg": "^8.6.4",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "eslint-auto-fix": "^1.0.3",
    "eslint-git-integration": "^1.1.0",
    "express": "^4.17.2",
    "express-sharp": "^4.2.12",
    "file-system": "^2.2.2",
    "jsonwebtoken": "^8.5.1",
    "npm-watch": "^0.11.0",
    "npmrc": "^1.1.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@mizdra/eslint-interactive": "^6.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.9",
    "@types/pg": "^8.6.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "body-parser": "^1.19.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-json-files": "^1.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.17.2",
    "husky": "^7.0.0",
    "jasmine": "^4.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "jest": "^27.4.7",
    "lint-staged": "12.2.2",
    "nodemon": "^2.0.15",
    "onchange": "^7.1.0",
    "prettier": "^2.5.1",
    "prettier-check": "^2.0.0",
    "sharp": "^0.29.3",
    "supertest": "^6.2.2",
    "ts": "^0.2.2",
    "typescript": "^4.5.4"
  }
}
